{
  "name": "hey-api-playwright",
  "version": "0.1.0",
  "description": "Generate Playwright E2E test fixtures and route mocks from OpenAPI specifications",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "scripts": {
    "build": "tsup",
    "test": "vitest",
    "lint": "eslint src --ext .ts --fix",
    "typecheck": "tsc --noEmit"
  },
  "keywords": [
    "openapi",
    "playwright",
    "e2e",
    "testing",
    "mocks",
    "fixtures",
    "hey-api"
  ],
  "author": "Jeffrey Nijs",
  "license": "MIT",
  "peerDependencies": {
    "@hey-api/openapi-ts": ">=0.61.0",
    "@playwright/test": ">=1.40.0"
  },
  "dependencies": {
    "hey-api-builders": "github:JeffreyNijs/hey-api-builders"
  },
  "devDependencies": {
    "@hey-api/openapi-ts": "^0.61.0",
    "@playwright/test": "^1.48.0",
    "@types/node": "^22.0.0",
    "@vitest/coverage-v8": "^2.1.9",
    "eslint": "^9.0.0",
    "tsup": "^8.0.0",
    "typescript": "^5.7.0",
    "vitest": "^2.0.0"
  }
}
